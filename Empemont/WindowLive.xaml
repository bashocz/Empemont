<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	x:Name="DialogLive"
	x:Class="Empemont.WindowLive"
	Title="Živé vysílání"
	Width="640" Height="480" mc:Ignorable="d" ScrollViewer.VerticalScrollBarVisibility="Disabled" WindowStartupLocation="CenterOwner">

	<Grid Background="{DynamicResource WindowBackground}">
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition Height="48"/>
		</Grid.RowDefinitions>
		<Grid x:Name="GridPage" Margin="8"/>
		<Grid Margin="0" Grid.Row="1">
			<Button x:Name="ButtonCancel" HorizontalAlignment="Right" Margin="0,8,8,8" Width="75" Height="24" Content="Storno" d:LayoutOverrides="HorizontalAlignment"/>
			<Button x:Name="ButtonNext" HorizontalAlignment="Right" Margin="0,8,112,8" Width="75" Height="24" Content="Další" d:LayoutOverrides="HorizontalAlignment"/>
            <Button x:Name="ButtonPrev" HorizontalAlignment="Right" Margin="0,8,200,8" Width="75" Height="24" Content="Předchozí" d:LayoutOverrides="HorizontalAlignment"/>
        </Grid>
	</Grid>
</Window>